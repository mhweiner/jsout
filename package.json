  "license": "MIT",
  "author": "Marc H. Weiner <mhweiner234@gmail.com> (https://mhweiner.com)",
  "main": "./dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!dist/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mhweiner/jsout.git"
  },
