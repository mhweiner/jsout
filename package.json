    "cross-platform",
    "browser",
    "bun",
    "deno",
    "nodejs",
    "zero-dependencies",
    "portable"
  ],
  "devDependencies": {
    "@types/node": "20.17.37",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "c8": "^10.1.2",
    "cjs-mock": "3.0.1",
    "eslint": "^8.4.1",
    "kizu": "3.8.0",
    "sinon": "^12.0.1",
    "ts-node": "10.9.2",
    "typescript": "5.8.3"
  }
